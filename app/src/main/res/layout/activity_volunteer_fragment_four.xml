<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ui.activity.Volunteer.VolunteerFragmentFourActivity">

    <TextView
        android:layout_width="200dp"
        android:layout_height="40dp"
        android:text="봉사시간"
        android:id="@+id/textView21"
        android:textColor="#000000"
        android:textSize="28dp"
        android:background="@drawable/login_edittext"
        android:layout_alignParentTop="true"
        android:layout_alignLeft="@+id/frameLayout3"
        android:layout_alignStart="@+id/frameLayout3"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="15dp" />

    <FrameLayout
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:id="@+id/frameLayout3"
        android:background="@drawable/volunteertimeframe"
        android:layout_below="@+id/textView21"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="20dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:id="@+id/textVolunteerTime"
            android:gravity="center_vertical|center_horizontal"
            android:layout_gravity="center"
            android:textSize="70dp"
            android:textColor="#000000" />
    </FrameLayout>

    <com.github.mikephil.charting.charts.BarChart
        android:id="@+id/volunteer_time_chart"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginTop="20dp"
        android:layout_below="@+id/frameLayout3"
        android:layout_marginBottom="90dp"
        android:layout_alignLeft="@+id/frameLayout3"
        android:layout_alignStart="@+id/frameLayout3"
        android:layout_alignRight="@+id/frameLayout3"
        android:layout_alignEnd="@+id/frameLayout3" />

</RelativeLayout>
